<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- define the app theme and globle animation -->
    <style name="BaseTheme" mce_bogus="1" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/activity_theme_animation</item>
        <item name="android:excludeFromRecents">true</item>
    </style>

    <style name="AppTheme" parent="BaseTheme">

    </style>

    <style name="search_theme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="ThemeWidthActionBar" parent="android:Theme.Holo.Light.DarkActionBar">

    </style>

    <style name="activity_theme_animation" mce_bogus="1" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/translate_between_interface_right_in
        </item>
        <item name="android:activityOpenExitAnimation">@anim/translate_between_interface_left_out
        </item>
        <item name="android:activityCloseEnterAnimation">@anim/translate_between_interface_left_in
        </item>
        <item name="android:activityCloseExitAnimation">
            @anim/translate_between_interface_right_out
        </item>
        <item name="android:taskOpenEnterAnimation">@anim/translate_between_interface_right_in
        </item>
        <item name="android:taskOpenExitAnimation">@anim/translate_between_interface_left_out</item>
        <item name="android:taskCloseEnterAnimation">@anim/translate_between_interface_left_in
        </item>
        <item name="android:taskCloseExitAnimation">@anim/translate_between_interface_right_out
        </item>
        <item name="android:taskToFrontEnterAnimation">@anim/translate_between_interface_right_in
        </item>
        <item name="android:taskToFrontExitAnimation">@anim/translate_between_interface_left_out
        </item>
        <item name="android:taskToBackEnterAnimation">@anim/translate_between_interface_left_in
        </item>
        <item name="android:taskToBackExitAnimation">@anim/translate_between_interface_right_out
        </item>
        <item name="android:wallpaperOpenEnterAnimation">
            @anim/translate_between_interface_right_in
        </item>
        <item name="android:wallpaperOpenExitAnimation">@anim/translate_between_interface_left_out
        </item>
        <item name="android:wallpaperCloseEnterAnimation">
            @anim/translate_between_interface_left_in
        </item>
        <item name="android:wallpaperCloseExitAnimation">
            @anim/translate_between_interface_right_out
        </item>
        <item name="android:wallpaperIntraOpenEnterAnimation">
            @anim/translate_between_interface_right_in
        </item>
        <item name="android:wallpaperIntraOpenExitAnimation">
            @anim/translate_between_interface_left_out
        </item>
        <item name="android:wallpaperIntraCloseEnterAnimation">
            @anim/translate_between_interface_left_in
        </item>
        <item name="android:wallpaperIntraCloseExitAnimation">
            @anim/translate_between_interface_right_out
        </item>
    </style>





