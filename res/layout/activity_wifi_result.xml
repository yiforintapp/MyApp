<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ripple="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <com.leo.appmaster.ui.CommonToolbar
        android:id="@+id/wifi_result_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:id="@+id/top_content"
        android:layout_width="match_parent"
        android:layout_height="155dp"
        android:layout_below="@id/wifi_result_title_bar"
        android:background="@color/cb"
        android:gravity="center">

        <ImageView
            android:id="@+id/wifi_big_icon"
            android:layout_width="93dp"
            android:layout_height="92dp"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/wifi_big_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/wifi_big_icon"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:text="@string/wifi_scan_result_safe"
            android:textColor="@color/white"
            android:textSize="17dp" />

        <TextView
            android:id="@+id/wifi_sma_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/wifi_big_text"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:text="@string/wifi_scan_result_no_safe_advice"
            android:textColor="@color/white"
            android:textSize="17dp"
            android:visibility="invisible" />
    </RelativeLayout>

    <!-- safe content-->
    <RelativeLayout
        android:id="@+id/bottom_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/top_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/wifi_result_1px_bg"
        android:paddingBottom="19dp"
        android:paddingLeft="19dp"
        android:paddingRight="19dp"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/wifi_result_top_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp">

            <TextView
                android:id="@+id/blue_tiao"
                android:layout_width="3dp"
                android:layout_height="14dp"
                android:layout_centerVertical="true"
                android:background="@color/wifi_result_blue" />

            <TextView
                android:id="@+id/text_flow_guanli"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/blue_tiao"
                android:text="@string/hp_device_gprs"
                android:textColor="@color/c2"
                android:textSize="15dp" />
        </RelativeLayout>

        <ImageView
            android:id="@+id/small_black_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/wifi_result_top_content"
            android:layout_marginTop="5dp"
            android:background="@color/c4" />

        <RelativeLayout
            android:id="@+id/low_bottom_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/small_black_line"
            android:layout_marginTop="19dp">

            <ImageView
                android:id="@+id/low_icon"
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:layout_centerVertical="true"
                android:background="@drawable/wifi_result_traffic" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="9dp"
                android:layout_toRightOf="@id/low_icon"
                android:text="@string/wifi_scan_result_traffic_text"
                android:textColor="@color/c2"
                android:textSize="14dp" />
        </RelativeLayout>

        <com.leo.appmaster.ui.RippleView
            android:id="@+id/wifi_resulte_sure"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_below="@id/low_bottom_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/green_radius_btn_shape"
            android:clickable="true"
            ripple:rippleColor="@color/green_button_ripple"
            ripple:rippleDuration="250"
            ripple:rippleHover="true"
            ripple:rippleOverlay="true">

            <TextView
                android:id="@+id/wifi_result_sure_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/wifi_scan_result_use_now"
                android:textColor="@color/white"
                android:textSize="16dp" />
        </com.leo.appmaster.ui.RippleView>
    </RelativeLayout>
    <!-- safe content-->

    <!-- un safe content-->
    <RelativeLayout
        android:id="@+id/unsafe_result_content"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_below="@id/top_content"
        android:layout_centerInParent="true"
        android:layout_marginTop="30dp"
        android:background="@color/white"
        android:paddingLeft="34dp"
        android:paddingRight="34dp">

        <RelativeLayout
            android:id="@+id/unsafe_wifi_is_connect"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/unsafe_wifi_is_connect_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/wifi_result_one"
                android:textColor="@color/c2"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/unsafe_wifi_is_connect_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/loading_bar" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/unsafe_wifi_second_connect"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/unsafe_wifi_is_connect"
            android:layout_marginTop="20dp">

            <TextView
                android:id="@+id/unsafe_wifi_second_connect_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/wifi_result_two"
                android:textColor="@color/c2"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/unsafe_wifi_second_connect_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/loading_bar" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/unsafe_wifi_ssl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/unsafe_wifi_second_connect"
            android:layout_marginTop="20dp">

            <TextView
                android:id="@+id/unsafe_wifi_ssl_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/wifi_result_three"
                android:textColor="@color/c2"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/unsafe_wifi_ssl_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/loading_bar" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/unsafe_wifi_pas_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/unsafe_wifi_ssl"
            android:layout_marginTop="20dp">

            <TextView
                android:id="@+id/unsafe_wifi_pas_type_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/wifi_result_four"
                android:textColor="@color/c2"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/unsafe_wifi_pas_type_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/loading_bar" />
        </RelativeLayout>
    </RelativeLayout>
    <!-- un safe content-->

    <!-- un safe bottom content-->
    <RelativeLayout
        android:id="@+id/wifi_below_view_recomment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:visibility="gone">

        <TextView
            android:id="@+id/wifi_view_line"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/pri_pro_divider" />


        <RelativeLayout
            android:id="@+id/wifi_success_lockRT"
            android:layout_width="match_parent"
            android:layout_height="63dp"
            android:layout_below="@id/view_line"
            android:gravity="center">

            <com.leo.appmaster.ui.RippleView
                android:id="@+id/wifi_resulte_other_wifi"
                android:layout_width="210dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:background="@drawable/green_radius_btn_shape"
                android:clickable="true"
                ripple:rippleColor="@color/green_button_ripple"
                ripple:rippleDuration="250"
                ripple:rippleHover="true"
                ripple:rippleOverlay="true">

                <TextView
                    android:id="@+id/wifi_recomment_lock"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/select_other_wifi"
                    android:textColor="@color/white"
                    android:textSize="16dp" />
            </com.leo.appmaster.ui.RippleView>
        </RelativeLayout>
    </RelativeLayout>
    <!-- un safe bottomcontent-->
</RelativeLayout>